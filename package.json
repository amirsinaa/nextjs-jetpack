{
  "name": "nextjs-jetpack",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:with-express": "ts-node --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build --profile",
    "build:with-express": "npm run build:next && npm run build:server",
    "build": "npm run build:next",
    "preview": "next start -p 4000",
    "preview:with-express": "NODE_ENV=production node dist/server/index.js",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev 3000 cypress:open",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "pm2-start:production": "pm2 start ecosystem.config.js --only nextjp && pm2 logs",
    "pm2-start:staging": "pm2 start ecosystem.config.js --only nextjp-staging-app && pm2 logs",
    "reload:production": "pm2 reload ecosystem.config.js --only nextjp",
    "reload:staging": "pm2 reload ecosystem.config.js --only nextjp-staging-app"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.1",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@tanstack/react-query": "^4.13.4",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "env-kit": "^2.0.4",
    "express": "^4.18.1",
    "formik": "^2.2.9",
    "framer-motion": "^7.3.2",
    "interweave": "^13.0.0",
    "lint-staged": "^13.0.3",
    "next": "^12.3.0",
    "next-seo": "^5.5.0",
    "next-translate": "^1.5.0",
    "normalize.css": "8.0.1",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.4.0",
    "sass": "1.54.9",
    "styled-components": "^5.3.5",
    "stylis": "^4.1.2",
    "stylis-plugin-rtl": "^2.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^4.0.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@tanstack/react-query-devtools": "^4.13.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.19",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.1",
    "eslint-config-next": "12.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "msw": "^0.47.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
